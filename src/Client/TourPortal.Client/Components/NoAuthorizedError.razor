@code {
    protected override async Task OnInitializedAsync()
    {
        var uri = NavigationManager.Uri.ToLower();
        if (uri.Contains("administrator"))
        {
            NavigationManager.NavigateTo("user/error/notauthorized");
        }
        else if (uri.Contains("user"))
        {
            NavigationManager.NavigateTo("administrator/error/notauthorized");
        }
        else
        {
            NavigationManager.NavigateTo("/error");
        }
    }
}